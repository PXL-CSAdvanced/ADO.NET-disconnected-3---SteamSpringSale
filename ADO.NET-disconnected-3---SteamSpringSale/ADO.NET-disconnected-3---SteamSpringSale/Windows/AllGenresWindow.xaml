<Window x:Class="ADO.NET_disconnected_3___SteamSpringSale.Windows.AllGenresWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADO.NET_disconnected_3___SteamSpringSale.Windows"
        mc:Ignorable="d"
        Title="All Genres"
        ResizeMode="CanResizeWithGrip"
        Style="{StaticResource WindowStyle}">
    <Grid>
        <ListBox Style="{StaticResource SteamListBoxStyle}" Name="genresListBox" Margin="20">
            <ListBoxItem>Action</ListBoxItem>
            <ListBoxItem>Puzzle</ListBoxItem>
        </ListBox>
    </Grid>
</Window>
